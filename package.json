{
  "name": "@singcl/throttle-debounce",
  "version": "1.1.0",
  "description": "throttle and debounce in browaser or building with module bundler",
  "main": "./index.js",
  "scripts": {
    "test": "karma start --single-run --browsers PhantomJS",
    "cov": "cat ./coverage/lcov/lcov.info | coveralls",
    "covv": "./node_modules/.bin/coveralls",
    "dev": "webpack ./index.js -o lib/bundle.js --mode development --output-library-target umd",
    "build": "webpack ./index.js -o lib/bundle.min.js --mode production --output-library-target umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/singcl/throttle-debounce.git"
  },
  "keywords": [
    "throttle",
    "debounce",
    "browser",
    "module"
  ],
  "author": "singcl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/singcl/throttle-debounce/issues"
  },
  "homepage": "https://github.com/singcl/throttle-debounce#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "karma": "^0.13.2",
    "karma-browserify": "^5.0.4",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha-reporter": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-qunit": "^0.1.9",
    "phantomjs-prebuilt": "^2.1.7",
    "qunitjs": "^1.23.1",
    "watchify": "^3.7.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
